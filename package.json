    "@types/react-dom": "^17.0.1",
    "apollo": "^2.32.5",
    "framer-motion": "^3.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.3",
    "typescript": "^4.2.2"
  },
